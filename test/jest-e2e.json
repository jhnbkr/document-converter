{
  "moduleFileExtensions": [
    "ts",
    "js",
    "json"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "forceExit": true,
  "detectOpenHandles": true,
  "moduleNameMapper": {
    "^@common/(.*)$": "<rootDir>/../src/common/$1",
    "^@document/(.*)$": "<rootDir>/../src/document/$1"
  }
}
